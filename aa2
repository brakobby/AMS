import customtkinter as ctk
win = ctk.CTk()
win.title("AMS")
win.geometry("1300x760")
win.resizable(0,0)
win._set_appearance_mode("light")





topnav2 = ctk.CTkFrame(win, fg_color="#084F43", height=70, width=1300, corner_radius=0)
topnav2.place(relx=0, rely=0.065)


bottomnav2 = ctk.CTkFrame(win, fg_color="#084F43", height=80, width=850, corner_radius=0)
bottomnav2.place(relx=0.25, rely=0.8)


b1 = ctk.CTkButton(bottomnav2, height=50, width=180, fg_color="#084F43",  text="SUBMIT", text_color="white", border_color="black", border_width=2)
b1.place(relx=0.1, rely=0.2)

b2 = ctk.CTkButton(bottomnav2, height=50, width=180, fg_color="#A20025",  text="RESET", text_color="white", border_color="black", border_width=2)
b2.place(relx=0.40, rely=0.2)

b3 = ctk.CTkButton(bottomnav2, height=50, width=180, fg_color="#009494",  text="UPLOAD", text_color="white", border_color="black", border_width=2)
b3.place(relx=0.70, rely=0.2)



sidenav = ctk.CTkFrame(win, fg_color="#0e716b", height=760, width=220, corner_radius=0)
sidenav.place(relx=0, rely=0)

